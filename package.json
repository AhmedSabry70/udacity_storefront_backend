{
  "name": "udacity_storefront_backend",
  "version": "1.9.2",
  "description": "frontbackstore",
  "main": "index.js",
  "author": "Ahmed Sabry",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' ./src/index.ts",
    "test": "npm run db-reset&&export NODE_ENV=test||set NODE_ENV=test && db-migrate up --env test&& tsc && jasmine && db-migrate db:drop test",
    "db-up": "db-migrate up",
    "db-reset": "db-migrate reset && db-migrate down",
    "teardown": "db-migrate reset --config ./database.json --e test",
    "lint": "npx eslint .",
    "lintfix": "eslint . --ext .ts --fix",
    "prettier": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/hapi__joi": "^17.1.8",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-rate-limit": "^6.2.1",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "supertest": "^6.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.17",
    "@types/pg": "^8.6.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^4.0.2",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
